{
  "$schema": "appsettings-schema.json",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    }
  },
  "Mikkel": {
    "AwesomeSiteEnabled":true
  },
  "Umbraco": {
    "CMS": {
      "Global": {
        "Id": "27c2286c-b90a-4112-b905-27293c5258db",
        "UseHttps": true,
        "NoNodesViewPath": "~/umbraco/UmbracoWebsite/Deploy/NoNodes.cshtml",
        "SanitizeTinyMce": true
      },
      "Content": {
        "AllowEditInvariantFromNonDefault": true,
        "ContentVersionCleanupPolicy": {
          "EnableCleanup": true
        }
      },
      "DeliveryApi": {
        "Enabled": false,
        "PublicAccess": false,
        "ApiKey": ""
      },
      "ModelsBuilder": {
        "ModelsMode": "SourceCodeAuto",
        "ModelsDirectory": "~/../Mikkelhm.Models",
        "AcceptUnsafeModelsDirectory": true,
        "ModelsNamespace": "Mikkelhm.Models"
      }
    },
    "Licenses": {
      "Products": {
        "Umbraco.Deploy": "UMBRACO-CLOUD",
        "Umbraco.Forms": "UMBRACO-CLOUD"
      }
    }
  }
}
